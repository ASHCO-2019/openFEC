{
  "devDependencies": {
    "swagger-ui-dist": "^3.13.4"
  },
  "scripts": {
    "build": "npm run remove-old-version && npm run copy-swagger-ui && npm run clean-files",
    "copy-swagger-ui": "mkdir -p ./static/swagger-ui/ && cp -R ./node_modules/swagger-ui-dist/ ./static/swagger-ui/",
    "clean-files": "rm -f ./static/swagger-ui/.npmignore ./static/swagger-ui/README.md ./static/swagger-ui/index.html ./static/swagger-ui/index.js ./static/swagger-ui/oauth2-redirect.html ./static/swagger-ui/package.json",
    "remove-old-version": "rm -rf ./static/swagger-ui/"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  }
}
